{
  "name": "StrimsLink",
  "version": "0.1",
  "description": "Replaces twitch-channel links to Strims-link instead.",
  "permissions": [
    "storage",
    "declarativeContent"
  ],
  "background": {
    "scripts": ["strims-link.js"],
    "persisten": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
